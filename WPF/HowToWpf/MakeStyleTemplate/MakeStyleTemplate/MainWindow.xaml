<Window x:Class="MakeStyleTemplate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MakeStyleTemplate"
        mc:Ignorable="d"
      
        Title="MainWindow" Height="450" Width="800"
       
        >
    <Window.Resources>
        <!-- Resources for the entire application can be defined here. -->
        <ResourceDictionary Source="../MakeStyleTemplate/Style/Dictionary1.xaml"/>
    </Window.Resources>

    <StackPanel>
        <Button Content = "Button1" Style="{StaticResource ResourceKey=roundbutton}"/>
        
    </StackPanel>
</Window>
